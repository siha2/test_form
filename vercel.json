{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.7.1"
    }
  },
  "routes": [
    { "src": "/favicon.ico", "dest": "/public/images/favicon.ico" },
    { "src": "/css/(.*)", "dest": "/public/css/$1" },
    { "src": "/js/(.*)", "dest": "/public/js/$1" },
    { "src": "/fonts/(.*)", "dest": "/public/fonts/$1" },
    { "src": "/(.*)", "dest": "/api/index.php" }
  ]
}
